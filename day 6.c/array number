 int arr[9];
 printf("enter the array:");
 for(int i=0;i<9;i++)
 {
     scanf("%d",&arr[i]);
     
}
    printf("\n");
    for(int i=0;i<9;i++)
    {
        printf("the arrays are:");
        printf("%d \n",arr[i]);
        
    }
    return 0;
}