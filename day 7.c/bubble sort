#include <stdio.h>
int main()
{
int temp;
int arr[5]={81,62,53,74,95};
for(int i=0;i<=5;i++)
{
    for(int j=i+1;j<=5;j++)
    {
        if(arr[i]>arr[j])
        {
            temp=arr[i];
            arr[i]=arr[j];
            arr[j]=temp;
        }
    }
}
    printf("sorted array:");
    for(int i=1;i<=5;i++)
    {
    printf("%d",arr[i]);
   
    }
    return 0;
}