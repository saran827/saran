#include<stdio.h>
int main()
{
   int n,prime=1;
   printf("enter the number:");
   scanf("%d",&n);
   for(int i=2;i<=n/2;i++)
   {
       if(n==0)
       {
           prime=0;
           break;
       }
   }
   if(prime==1)
   {
       printf("%d it is prime number:\n",prime);
    }
    else{
        printf("%d it is not prime number:\n",prime);
    }
    return 0;
    
    
}